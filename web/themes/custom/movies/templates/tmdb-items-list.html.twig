<div class="items-list-wrapper">
  <div class="container">

    {% if title %}
      <div class="items-list-title">
        {{ title }}
      </div>
    {% endif %}

    <div class="items-list-content row">
      {% for item in items %}
        <div class="col-lg-2 col-md-3 col-sm-4 col-6">
          {{ item }}
        </div>
      {% endfor %}
    </div>

  </div>
</div>
